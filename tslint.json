{
    "extends": [
        "tslint:latest",
        "tslint-config-prettier",
        "tslint-plugin-prettier"
    ],
    "rules": {
        "prettier": [true, ".prettierrc.js", { "editorconfig": false }],
        "no-console": false,
        "interface-over-type-literal": false,
        "object-literal-sort-keys": false
    }
}
